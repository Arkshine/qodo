{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "lib/*": ["lib/*"],
      "api/*": ["src/api/*"],
      "vue$": ["node_modules/vue/dist/vue.esm.js"]
    }
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ]
}
